    </fields>
    <use_formatting>Y</use_formatting>
    <attributes/>
    <GUI>
      <xloc>992</xloc>
      <yloc>144</yloc>
    </GUI>
  </transform>
  <transform>
    <name>set end loop</name>
    <type>SetVariable</type>
