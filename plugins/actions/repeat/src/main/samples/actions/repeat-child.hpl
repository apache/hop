  <order>
    <hop>
      <from>generate dummy row</from>
      <to>get counter variable</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>counter + 1</from>
      <to>counter >5</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>counter >5</from>
      <to>set counter variable</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>get counter variable</from>
      <to>write counter to log</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>write counter to log</from>
      <to>counter + 1</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>counter >5</from>
      <to>set end loop</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>counter + 1</name>
    <type>Calculator</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
