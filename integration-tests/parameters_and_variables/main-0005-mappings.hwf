      <description/>
      <type>EVAL</type>
      <attributes/>
      <script>
var txt = previous_result.getLogText();


var ok = true;

var expectedVariables = [ 
   [ "TEST5_VARIABLE1",    "Mapping Variables.0 - TEST5_VARIABLE1 = >test5_value1&lt;",              "test5_value1"             ],
   [ "TEST5_PARAMETER1",   "Mapping Variables.0 - TEST5_PARAMETER1 = >test4_parameter1_value&lt;",   "test4_parameter1_value"   ],
   [ "TEST5_PARAMETER2",   "Mapping Variables.0 - TEST5_PARAMETER2 = >test5_default5&lt;", "default_test4_parameter2" ]
   
 ];

for (var i = 0 ; i&lt;expectedVariables.length ; i++) {
  var expectedVariable = expectedVariables[i];
  if ( !txt.contains(expectedVariable[1])) {
    ok = false;
    log.logError("Expected variable '"+expectedVariable[0]+"' to be set to '"+expectedVariable[2]+"' at least once");
  }
}

var expectedValues = [ 
   [ "param1", "Mapping Variables.0 - param1 = test4_parameter1_value",  "test4_parameter1_value" ],
   [ "param2", "Mapping Variables.0 - param2 = test5_default5",          "test5_default5"         ],
   [ "value1", "Mapping Variables.0 - value1 = test5_value1",            "test5_value1"           ]
 ];

for (var i = 0 ; i&lt;expectedValues.length ; i++) {
  var expectedValue = expectedValues[i];
  if ( !txt.contains(expectedValue[1])) {
    ok = false;
    log.logError("Expected field '"+expectedValue[0]+"' to be set to '"+expectedValue[2]+"'");
  }
}


ok;</script>
