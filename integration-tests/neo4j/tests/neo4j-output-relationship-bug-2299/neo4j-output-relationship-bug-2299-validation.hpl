<pipeline>
  <info>
    <name>neo4j-output-relationship-bug-2299-validation</name>
    <name_sync_with_filename>Y</name_sync_with_filename>
    <description>Validation pipeline for relationship bug test (#2299)</description>
    <extended_description>Validates that relationships are created with correct types when relationship type changes within a batch. The Cypher query verifies:
- All 6 expected relationships exist
- Each relationship has the correct type (HAS_ORDER, HAS_DELIVERABLE, HAS_WORK)
- The critical test: PROJECT->DELIVERABLE should have HAS_DELIVERABLE (not HAS_ORDER from first row)
- PROJECT->WORK should have HAS_WORK (not HAS_ORDER from first row)</extended_description>
    <pipeline_version/>
    <pipeline_type>Normal</pipeline_type>
    <parameters>
    </parameters>
    <capture_transform_performance>N</capture_transform_performance>
    <transform_performance_capturing_delay>1000</transform_performance_capturing_delay>
    <transform_performance_capturing_size_limit>100</transform_performance_capturing_size_limit>
    <created_user>-</created_user>
    <created_date>2025/12/11 00:00:00.000</created_date>
    <modified_user>-</modified_user>
    <modified_date>2025/12/11 00:00:00.000</modified_date>
  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>Validate Relationships</from>
      <to>OUTPUT</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>OUTPUT</name>
    <type>Dummy</type>
    <description>Output transform - test framework validates row count and data</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <attributes/>
    <GUI>
      <xloc>384</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Validate Relationships</name>
    <type>Neo4jCypherOutput</type>
    <description>Query all relationships and verify they have correct types</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Test</connection>
    <cypher>// Validate all relationships have been created with correct types
// Expected relationships:
// 1. PROJECT -> ORDER : HAS_ORDER
// 2. PROJECT -> DELIVERABLE : HAS_DELIVERABLE (critical - was bugged to use HAS_ORDER)
// 3. PROJECT -> WORK : HAS_WORK (critical - was bugged to use HAS_ORDER)
// 4. ORDER -> DELIVERABLE : HAS_DELIVERABLE
// 5. ORDER -> WORK : HAS_WORK
// 6. DELIVERABLE -> WORK : HAS_WORK

MATCH (from:NODE)-[r]->(to:NODE)
RETURN from.id as from_id,
       to.id as to_id,
       type(r) as rel_type
ORDER BY from.id, to.id, type(r)</cypher>
    <cypher_from_field>N</cypher_from_field>
    <mappings>
</mappings>
    <read_only>Y</read_only>
    <retry>Y</retry>
    <returning_graph>N</returning_graph>
    <returns>
      <return>
        <name>from_id</name>
        <source_type>String</source_type>
        <type>String</type>
      </return>
      <return>
        <name>to_id</name>
        <source_type>String</source_type>
        <type>String</type>
      </return>
      <return>
        <name>rel_type</name>
        <source_type>String</source_type>
        <type>String</type>
      </return>
    </returns>
    <unwind>N</unwind>
    <attributes/>
    <GUI>
      <xloc>64</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
