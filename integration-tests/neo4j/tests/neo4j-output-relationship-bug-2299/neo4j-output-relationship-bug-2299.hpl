<pipeline>
  <info>
    <name>neo4j-output-relationship-bug-2299</name>
    <name_sync_with_filename>Y</name_sync_with_filename>
    <description>Test pipeline to reproduce issue #2299: incorrect relationships when relationship value changes in batch</description>
    <extended_description/>
    <pipeline_version/>
    <pipeline_type>Normal</pipeline_type>
    <parameters>
    </parameters>
    <capture_transform_performance>N</capture_transform_performance>
    <transform_performance_capturing_delay>1000</transform_performance_capturing_delay>
    <transform_performance_capturing_size_limit>100</transform_performance_capturing_size_limit>
    <created_user>-</created_user>
    <created_date>2025/12/11 00:00:00.000</created_date>
    <modified_user>-</modified_user>
    <modified_date>2025/12/11 00:00:00.000</modified_date>
  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>Test Data</from>
      <to>Neo4j Output</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>Neo4j Output</name>
    <type>Neo4JOutput</type>
    <description>Neo4j Output transform with batch size 2 to test relationship batching bug</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <batch_size>4</batch_size>
    <connection>Test</connection>
    <create_indexes>N</create_indexes>
    <from>
      <labels>
        <label/>
        <value>NODE</value>
      </labels>
      <properties>
        <property>
          <name>id</name>
          <primary>Y</primary>
          <type>String</type>
          <value>from_node_id</value>
        </property>
      </properties>
      <read_only_from_node>N</read_only_from_node>
    </from>
    <only_create_relationships>N</only_create_relationships>
    <relationship>relationship_type</relationship>
    <relationship_value/>
    <relprops/>
    <return_graph_field>graph</return_graph_field>
    <returning_graph>N</returning_graph>
    <to>
      <labels>
        <label/>
        <value>NODE</value>
      </labels>
      <properties>
        <property>
          <name>id</name>
          <primary>Y</primary>
          <type>String</type>
          <value>to_node_id</value>
        </property>
      </properties>
      <read_only_to_node>N</read_only_to_node>
    </to>
    <use_create>N</use_create>
    <attributes/>
    <GUI>
      <xloc>384</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Test Data</name>
    <type>DataGrid</type>
    <description>Test data with changing relationship types WITHIN a batch to reproduce the bug. With batch_size=4, the first batch contains rows with different relationship types (HAS_ORDER, HAS_DELIVERABLE, HAS_WORK). Before the fix, all relationships in this batch would incorrectly use HAS_ORDER (from the first row).</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <data>
      <line>
        <item>PROJECT</item>
        <item>ORDER</item>
        <item>HAS_ORDER</item>
      </line>
      <line>
        <item>PROJECT</item>
        <item>DELIVERABLE</item>
        <item>HAS_DELIVERABLE</item>
      </line>
      <line>
        <item>PROJECT</item>
        <item>WORK</item>
        <item>HAS_WORK</item>
      </line>
      <line>
        <item>ORDER</item>
        <item>DELIVERABLE</item>
        <item>HAS_DELIVERABLE</item>
      </line>
      <line>
        <item>ORDER</item>
        <item>WORK</item>
        <item>HAS_WORK</item>
      </line>
      <line>
        <item>DELIVERABLE</item>
        <item>WORK</item>
        <item>HAS_WORK</item>
      </line>
    </data>
    <fields>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>from_node_id</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>to_node_id</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>relationship_type</name>
        <type>String</type>
      </field>
    </fields>
    <attributes/>
    <GUI>
      <xloc>128</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
