<pipeline>
  <info>
    <name>neo4j-output-array-support-3552-validation</name>
    <name_sync_with_filename>Y</name_sync_with_filename>
    <description>Validation pipeline for Array type support test</description>
    <extended_description>Validates that Array type properties were correctly written to Neo4j nodes</extended_description>
    <pipeline_version/>
    <pipeline_type>Normal</pipeline_type>
    <parameters>
    </parameters>
    <capture_transform_performance>N</capture_transform_performance>
    <transform_performance_capturing_delay>1000</transform_performance_capturing_delay>
    <transform_performance_capturing_size_limit>100</transform_performance_capturing_size_limit>
    <created_user>-</created_user>
    <created_date>2025/12/11 00:00:00.000</created_date>
    <modified_user>-</modified_user>
    <modified_date>2025/12/11 00:00:00.000</modified_date>
  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>Neo4j Cypher</from>
      <to>Dummy (do nothing)</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>Dummy (do nothing)</name>
    <type>Dummy</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <attributes/>
    <GUI>
      <xloc>320</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Neo4j Cypher</name>
    <type>Neo4jCypherOutput</type>
    <description>Query Document nodes with Array properties</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Test</connection>
    <cypher>MATCH (d:Document)
RETURN d.id as id,
       d.title as title,
       size(d.embedding) as embedding,
       size(d.tags) as tags
ORDER BY d.id</cypher>
    <cypher_from_field>N</cypher_from_field>
    <mappings>
</mappings>
    <read_only>Y</read_only>
    <retry>Y</retry>
    <returning_graph>N</returning_graph>
    <returns>
      <return>
        <name>id</name>
        <source_type>String</source_type>
        <type>String</type>
      </return>
      <return>
        <name>title</name>
        <source_type>String</source_type>
        <type>String</type>
      </return>
      <return>
        <name>embedding</name>
        <source_type>Integer</source_type>
        <type>Integer</type>
      </return>
      <return>
        <name>tags</name>
        <source_type>Integer</source_type>
        <type>Integer</type>
      </return>
    </returns>
    <unwind>N</unwind>
    <attributes/>
    <GUI>
      <xloc>64</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
