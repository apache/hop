<pipeline>
  <info>
    <name>neo4j-gencsv-null-values-2950</name>
    <name_sync_with_filename>Y</name_sync_with_filename>
    <description>Test pipeline to reproduce issue #2950: Generate CSV errors on NULL values</description>
    <extended_description>This pipeline tests the Generate CSV transform with NULL values in properties. The current implementation fails with NullPointerException when properties have NULL values. After the fix, this should complete successfully.</extended_description>
    <pipeline_version/>
    <pipeline_type>Normal</pipeline_type>
    <parameters>
    </parameters>
    <capture_transform_performance>N</capture_transform_performance>
    <transform_performance_capturing_delay>1000</transform_performance_capturing_delay>
    <transform_performance_capturing_size_limit>100</transform_performance_capturing_size_limit>
    <created_user>-</created_user>
    <created_date>2025/12/11 00:00:00.000</created_date>
    <modified_user>-</modified_user>
    <modified_date>2025/12/11 00:00:00.000</modified_date>
  </info>
  <notepads>
  </notepads>
  <order>
    <hop>
      <from>Test Data with NULLs</from>
      <to>Create-Graph-Data</to>
      <enabled>Y</enabled>
    </hop>
    <hop>
      <from>Create-Graph-Data</from>
      <to>Generate CSV</to>
      <enabled>Y</enabled>
    </hop>
  </order>
  <transform>
    <name>Create-Graph-Data</name>
    <type>Neo4JOutput</type>
    <description>Create graph data from input rows - returns graph field for Generate CSV</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <array_separator>,</array_separator>
    <batch_size>1000</batch_size>
    <connection>Test</connection>
    <create_indexes>N</create_indexes>
    <from>
      <labels>
        <value>Person</value>
      </labels>
      <properties>
        <property>
          <name>id</name>
          <primary>Y</primary>
          <type>String</type>
          <value>person_id</value>
        </property>
        <property>
          <name>first_name</name>
          <primary>N</primary>
          <type>String</type>
          <value>first_name</value>
        </property>
        <property>
          <name>last_name</name>
          <primary>N</primary>
          <type>String</type>
          <value>last_name</value>
        </property>
        <property>
          <name>mail_addr1</name>
          <primary>N</primary>
          <type>String</type>
          <value>mail_addr1</value>
        </property>
        <property>
          <name>mail_addr2</name>
          <primary>N</primary>
          <type>String</type>
          <value>mail_addr2</value>
        </property>
        <property>
          <name>mail_addr3</name>
          <primary>N</primary>
          <type>String</type>
          <value>mail_addr3</value>
        </property>
        <property>
          <name>phone</name>
          <primary>N</primary>
          <type>String</type>
          <value>phone</value>
        </property>
      </properties>
      <read_only_from_node>N</read_only_from_node>
    </from>
    <only_create_relationships>N</only_create_relationships>
    <relprops>
</relprops>
    <return_graph_field>graph</return_graph_field>
    <returning_graph>Y</returning_graph>
    <to>
      <labels>
        <value>Person</value>
      </labels>
      <properties>
        <property>
          <name>id</name>
          <primary>Y</primary>
          <type>String</type>
          <value>person_id</value>
        </property>
      </properties>
      <read_only_to_node>N</read_only_to_node>
    </to>
    <use_create>N</use_create>
    <attributes/>
    <GUI>
      <xloc>288</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Generate CSV</name>
    <type>Neo4jLoad</type>
    <description>Generate CSV files from graph data - should handle NULL values without error</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <base_folder>/tmp/neo4j-import-test</base_folder>
    <file_type_field>file_type</file_type_field>
    <filename_field>filename</filename_field>
    <files_prefix>test-null-values</files_prefix>
    <graph_field_name>graph</graph_field_name>
    <uniqueness_strategy>None</uniqueness_strategy>
    <attributes/>
    <GUI>
      <xloc>480</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform>
    <name>Test Data with NULLs</name>
    <type>DataGrid</type>
    <description>Test data with NULL values in various fields to trigger the bug</description>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <data>
      <line>
        <item>PERSON001</item>
        <item>John</item>
        <item>Doe</item>
        <item>123 Main St</item>
        <item>Anytown</item>
        <item>USA</item>
        <item>555-0100</item>
      </line>
      <line>
        <item>PERSON002</item>
        <item>Jane</item>
        <item>Smith</item>
        <item>456 Oak Ave</item>
        <item>Springfield</item>
        <item>USA</item>
        <item/>
      </line>
      <line>
        <item>PERSON003</item>
        <item>Bob</item>
        <item>Williams</item>
        <item>789 Pine Rd</item>
        <item>Toronto</item>
        <item>Canada</item>
        <item/>
      </line>
      <line>
        <item>PERSON004</item>
        <item>Alice</item>
        <item>Brown</item>
        <item/>
        <item>Vancouver</item>
        <item>Canada</item>
        <item>555-0200</item>
      </line>
    </data>
    <fields>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>person_id</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>first_name</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>last_name</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>mail_addr1</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>mail_addr2</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>mail_addr3</name>
        <type>String</type>
      </field>
      <field>
        <length>-1</length>
        <precision>-1</precision>
        <set_empty_string>N</set_empty_string>
        <name>phone</name>
        <type>String</type>
      </field>
    </fields>
    <attributes/>
    <GUI>
      <xloc>96</xloc>
      <yloc>112</yloc>
    </GUI>
  </transform>
  <transform_error_handling>
  </transform_error_handling>
  <attributes/>
</pipeline>
