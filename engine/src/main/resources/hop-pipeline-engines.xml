<?xml version="1.0" encoding="UTF-8"?>
<hop-engines>

  <hop-engine id="Local">
    <description>Local pipeline engine</description>
    <tooltip>Executes your pipeline locally in a multi-threaded fashion</tooltip>
    <classname>org.apache.hop.pipeline.engines.local.LocalPipelineEngine</classname>
  </hop-engine>

  <hop-engine id="LocalSingle">
    <description>Local single threaded pipeline engine</description>
    <tooltip>Executes your pipeline locally in a single-threaded fashion</tooltip>
    <classname>org.apache.hop.pipeline.engines.localsingle.LocalSinglePipelineEngine</classname>
  </hop-engine>

</hop-engines>
